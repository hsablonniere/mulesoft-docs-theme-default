<main role="main" class="main-wrapper">
  <div class="navigation-wrapper">
    <nav role="navigation" class="navigation">
      <div class="search">
        <input type="text" placeholder="Search the docs" class="search-input" autocomplete="off" autocorrect="off" autocapitalize="off">
      </div>
      <div class="inner-nav">
        {{> navigation }}
      </div>
    </nav>
  </div>
  <article class="primary-content">
    <header class="article-header">
      {{>domain-version-selector}}
      <div class="breadcrumb">
        {{#each breadcrumbs }}
          <a class="breadcrumb-item" href="{{this.href}}">{{{this.label}}}</a> /
        {{/each}}
        <span class="breadcrumb-item">{{title}}</span>
      </div>
      <div class="page-version-selector">
        {{#each otherVersions }}
          <a class="page-version{{#equals this.version ../version}} page-version--current{{/equals}}" href="{{this.href}}">{{{this.version}}}</a>
        {{/each}}
      </div>
      <div class="edit-on-github">
        <a href="{{github-edit-url}}" class="edit-on-github-link">Edit on Github</a>
      </div>
    </header>
    <h1>{{title}}</h1>
    {{{contents}}}
  </article>
</main>
